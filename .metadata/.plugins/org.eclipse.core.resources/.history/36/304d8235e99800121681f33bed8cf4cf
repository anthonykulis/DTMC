#include "graph.h"

typedef struct {

	int north;
	int south;
	int west;
	int east;

} _g_node;

enum heading { NORTH, SOUTH, EAST, WEST };

int _g_heading = NORTH;
int _g_stack_ref = 0;

_g_node Graph[_G_STACK_SIZE];

void _g_push(int north, int east, int west, int south){

	int gsr = _g_stack_ref;
	Graph[gsr].north = north;
	Graph[gsr].south = south;
	Graph[gsr].east = east;
	Graph[gsr].west = west;
	_g_stack_ref = _g_stack_ref + 1) % _G_STACK_SIZE;

}
void _g_pop(){ _g_stack_ref--; }
int get_next_direction(int north, int east, int west, int south){}
