#include <bcm2835.h>

#define CR1 	0x20
#define CR2 	0x21
#define CR3 	0x22
#define CR4 	0x23
#define X_LSB 	0x28
#define X_MSB 	0x29
#define Y_LSB 	0x2A
#define Y_MSB 	0x2B
#define Z_LSB	0x2C
#define Z_MSB 	0x2D

