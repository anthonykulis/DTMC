#include "graph.h"

#include <stdlib.h>
#include <stdio.h>

int main(){

	fprintf(stdout, "Pushing  E1 S1 W1 N1\n");
	int dir = get_next_direction(1, 1, 1, 1);
	fprintf(stdout, "stack pointer: %d\n", get_sr());
	fprintf(stdout, "Resulting direction: %d\n\n", dir);

	fprintf(stdout, "Pushing  E1 S1 W1 N1\n");
	dir = get_next_direction(1, 1, 1, 1);
	fprintf(stdout, "stack pointer: %d\n", get_sr());
	fprintf(stdout, "Resulting direction: %d\n\n", dir);

	fprintf(stdout, "Pushing  E1 S0 W1 N1\n");
	dir = get_next_direction(1, 0, 1, 1);
	fprintf(stdout, "stack pointer: %d\n", get_sr());
	fprintf(stdout, "Resulting direction: %d\n", dir);



	return EXIT_SUCCESS;

}
