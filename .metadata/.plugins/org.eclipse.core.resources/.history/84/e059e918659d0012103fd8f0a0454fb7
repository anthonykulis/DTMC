#ifndef L3G_H_
#define L3G_H_

#define WHOAMI 0x0F 			//read only
#define CTRL_REG_1 0x20			//read|write
#define CTRL_REG_2 0x21			//read|write
#define CTRL_REG_3 0x22			//read|write
#define CTRL_REG_4 0x23			//read|write
#define CTRL_REG_5 0x24			//read|write

#define OUT_TEMP 0x36			//read only - gets temperature

#endif /* L3G_H_ */
